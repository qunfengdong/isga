<%args>
 @sorted
</%args>
<div class="list">
<table width="100%">
<thead>
<tr><th width="30%">Name</th><th>Value</th></tr>
</thead>
<tbody>

% foreach ( @sorted ) {
%   my ($var, $config) = @$_;
<tr>
 <td><strong><% $var->getName %></strong></td>
 <td><% $config->getValue %></td>
</tr>
<tr><td colspan="2" ><div class="documentation"><% $var->getDescription %></div></td></tr>
% }

</tbody>
</table>
</div>
