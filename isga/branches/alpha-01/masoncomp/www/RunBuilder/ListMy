<%args> # -*- cperl -*-
 $current_page => 1
</%args>

<%perl>
my $pager = SysMicro::Pager->new(Current => $current_page,
				 PerPage => 20);

my $account = SysMicro::Login->getAccount;
my $rbs = SysMicro::RunBuilder->query( CreatedBy => $account, OrderBy => 'StartedAt',
   				       Pager => $pager );   
</%perl>

<& /RunBuilder/list_my.mas, account => $account, pager => $pager, rbs => $rbs &>
