<%perl> # -*- cperl -*-

# check that we have a form cached and it is the correct type
my $form = $ISGA::WebApp->saved_args->{'form'};

$form and $form->get_formname() eq 'account_edit_my_details' 
  or $form =  ISGA::FormEngine::Account->EditMyDetails(\%ARGS);

return $m->comp('/Account/edit_my_details.mas', form => $form);

</%perl>
