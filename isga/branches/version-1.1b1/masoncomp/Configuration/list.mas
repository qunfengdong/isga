<%args>
 @sorted
</%args>
<div class="list">
<table width="100%">
<thead>
<tr><th width="25%">Name</th><th width="27%">Value</th><th width="48%">Description</th></tr>
</thead>
<tbody>

% foreach ( @sorted ) {
%   my ($var, $config) = @$_;
<tr>
 <td><% $var->getName %></td>
 <td><% $config->getValue %></td>
 <td><% $var->getDescription %></td>
</tr>
% }

</tbody>
</table>
</div>
