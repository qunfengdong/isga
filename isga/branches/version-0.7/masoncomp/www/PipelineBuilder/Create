<%perl> # -*- cperl -*-

# check that we have the correct form cached
my $form = $SysMicro::WebApp->saved_args->{'form'};
$form and $form->get_formname() eq 'pipeline_builder_create'
  or $form = SysMicro::FormEngine::PipelineBuilder->Create(\%ARGS);

return $m->comp('/PipelineBuilder/create.mas', form => $form);
</%perl>
