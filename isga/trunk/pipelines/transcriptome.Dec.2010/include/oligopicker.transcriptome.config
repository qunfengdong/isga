[interface]
classification = utility

[parameters]
;; 1 - 5
$;PROBE_COPY$;=___probecopy___
;; 10 - 70
$;LENGTH$;=___probelength___
;; 1 - 30
$;Tm$;=___tm___
;; 11 - 20
$;CROSS_REACTIVITY$;=___reactivity___
;; threshold BLAST score 20 - 50 
$;THRESHOLD$;=___threshold___
;; preferred end that oligos will be designed from 3 or 5
$;END$;=___end___
;; include filter sequence y or n
$;FILTER$;=n
;; design cross-reacting probes for sequences that can't be represented by unique probes? y or n
$;CROSS_REACTING_PROBES$;=n
;; output file name
$;OUTPUT$;=oligoOutput.xls

$;PROJECT_ABBREVIATION$;=$;PROJECT$;

[input]
$;INPUT_FILE_LIST$;=$;REPOSITORY_ROOT$;/output_repository/remove_short_sequence/$;PIPELINEID$;_default/remove_short_sequence.fsa.list
$;INPUT_FILE$;=
$;INPUT_DIRECTORY$;=
;; the following is only used when iterating over an INPUT_DIRECTORY
$;INPUT_EXTENSION$;=fsa


[output]
$;OUTPUT_TOKEN$;=transcriptome
$;OUTPUT_DIRECTORY$;=$;REPOSITORY_ROOT$;/output_repository/$;COMPONENT_NAME$;/$;PIPELINEID$;_$;OUTPUT_TOKEN$;
$;BSML_OUTPUT_LIST$;=$;OUTPUT_DIRECTORY$;/$;COMPONENT_NAME$;.bsml.list
$;RAW_OUTPUT_LIST$;=$;OUTPUT_DIRECTORY$;/$;COMPONENT_NAME$;.raw.list

[component]
$;COMPONENT_NAME$;=oligopicker
$;DESCRIPTION$;=none
$;WORKFLOW_REPOSITORY$;=$;REPOSITORY_ROOT$;/workflow/runtime/$;COMPONENT_NAME$;/$;PIPELINEID$;_$;OUTPUT_TOKEN$;
$;PIPELINE_TOKEN$;=unnamed

;The version,revision,tag here is set by an interpolated CVS tag
$;VERSION$;=2.0
$;RELEASE_TAG$;=$Name$
$;REVISION$;=$Revision: 5257 $

$;TEMPLATE_XML$;=$;DOCS_DIR$;/$;COMPONENT_NAME$;.xml
$;ITERATOR1$;=i1
$;ITERATOR1_XML$;=$;DOCS_DIR$;/$;COMPONENT_NAME$;.$;ITERATOR1$;.xml

;Distributed options
$;GROUP_COUNT$;=150
$;NODISTRIB$;=0

;the following keys are replaced at runtime by the invocation script
$;COMPONENT_CONFIG$;=
$;COMPONENT_XML$;=
$;PIPELINE_XML$;=
$;PIPELINEID$;=

[include]
$;PROJECT_CONFIG$;=


