<%args>
 $run
</%args>

% my $contigs = ISGA::Contig->query( Run => $run );


<form method="post" action="/Browser/gbrowse/<% $run %>/#search">

<select name="name">
% foreach ( @$contigs ) {
<option><% $_->getUniqueName %></option>
% }
</select>

<input type="submit" name="Select Contig" value="Select Contig" />

</form>
