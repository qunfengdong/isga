<%args> # -*- cperl -*-
	$blout
	$sgejob
</%args>

<%perl>
# check that we have a form cached and it is the correct type
#my $form = $SysMicro::WebApp->saved_args->{'form'};
#$form and $form->get_formname() eq 'workbench_blast'
#	or $form =  SysMicro::FormEngine::WorkBench->Blast(\%ARGS);
$sgejob->getCreatedBy == SysMicro::Login->getAccount
       or X::User::Denied->throw;
my $status = $sgejob->getStatus;

if($status eq "Finished"){
	return $m->comp('/WorkBench/Results/blast.mas', blout => $blout );
} elsif ($status eq "Running"){
	return $m->comp('/WorkBench/Results/running.mas');
} elsif ($status eq "Error"){

}

</%perl>
