<%args>
  @pipelines
</%args>

<h1>Public Pipelines</h1>

<p>The following pipelines are available to all users.</p>

<table width="100%">

% foreach ( @pipelines ) {
<tr>
 <th><% $_->getName %></th>
</tr>
<tr>
 <td><% $_->getDescription %><a class="button" href="/Pipeline/View?pipeline=<% $_ %>">View Details</a><br><br></td>
</tr>
% }

</table>
