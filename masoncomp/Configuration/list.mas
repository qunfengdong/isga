<%args>
 @sorted
</%args>
<div class="list">
<table width="100%">
<thead>
<tr><th width="30%">Name</th><th>Value</th></tr>
</thead>
<tbody>

% foreach ( @sorted ) {
% use Data::Dumper;
% warn Dumper($_);

%   my $var = $_->getVariable();

<tr>
 <td><strong><% $var->getName %></strong></td>
 <td><% $_->getValue %></td>
</tr>
<tr><td colspan="2" class="document"><% $var->getDescription %></td></tr>
% }

</tbody>
</table>
</div>
